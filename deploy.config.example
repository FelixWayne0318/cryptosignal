TELEGRAM_ENABLED=false
TELEGRAM_BOT_TOKEN=
TELEGRAM_CHAT_ID=

# ============================================
# 4. 服务器配置
# ============================================
SERVER_TIMEZONE=Asia/Singapore
PYTHON_VERSION=3.10

# ============================================
# 5. 服务器连接信息
# ============================================
SERVER_IP=YOUR_SERVER_IP_HERE
SERVER_USER=root
SERVER_PORT=22
